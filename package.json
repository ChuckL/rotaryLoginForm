{
  "name": "rotarylogin",
  "version": "1.0.0",
  "description": "Create a hero's path in google maps using timeline export",
  "main": "index.js",
  "scripts": {
    "start": "npx http-server ./dist",
    "build": "npx webpack",
    "build:html": "npm run tailwind && copyfiles src/* -e \"**/*.css\" -e \"**/*.ts\" ./dist --up 1",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --require ts-node/register 'src/**/*.test.ts'",
    "test:ci": "nyc npm test",
    "watch": "nodemon --watch src -e ts,html,css --exec \"npm run build\" ",
    "watch:html": "nodemon --watch src -e html,css --exec \"npm run build:html\" ",
    "tailwind": "npx tailwindcss -i ./src/style.css -o ./dist/style.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-almost": "^1.0.1",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "chai-almost": "^1.0.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "expect.js": "^0.3.1",
    "gulp": "^4.0.2",
    "http-server": "^14.1.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "ts-loader": "^9.2.5",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.4.0",
    "tsify": "^5.0.4",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.6.7",
    "@types/google.maps": "^3.45.6",
    "@types/lodash": "^4.14.172",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.10.6",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "object-hash": "^2.2.0",
    "rxjs": "^7.4.0",
    "spherical-geometry-js": "^3.0.0",
    "tailwindcss": "^3.0.7",
    "tw-elements": "^1.0.0-alpha12",
    "unzipit": "^1.3.5"
  }
}
